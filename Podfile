# platform :ios, '9.0'

target 'Relief' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Relief
	pod 'Firebase/Core'
	pod ‘Firebase/Auth’
	pod ‘Firebase/Database’
	pod ‘Firebase/Storage’
	pod ‘FacebookLogin’
	pod ‘FBSDKLoginKit’
    pod 'SwiftKeychainWrapper'
    pod 'Cosmos'
	pod 'Fabric'
	pod 'Crashlytics'

end
